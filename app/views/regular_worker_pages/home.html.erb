<% provide(:title, "Regular Worker") %>
<% provide(:subpage_title, "Home") %>
<h1>RegularWorkerPages#home</h1>
<p>Find me in app/views/regular_worker_pages/home.html.erb</p>


<div>
  <% if Micropost.find_all_by_regular(true).any? %>
  <h3> Posts (<%= Micropost.all.count %>)</h3>
  <ol class="microposts">
   
    <%= render @microposts %>
  </ol>
  <%= will_paginate @microposts %>
  <% end %>
</div>
